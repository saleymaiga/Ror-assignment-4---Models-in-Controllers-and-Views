<h1>sign up </h1>

<%= form_for(@user) do |f| %>
	<%= f.label :fname , "First Name" %>
	<%= f.text_field :fname %> <br>
	<%= f.label :lname, "Last Name" %>
	<%= f.text_field :lname %><br>
	<%= f.label :email, "Email address" %>
	<%= f.text_field :email %><br>

	<%= f.submit :submit %>	
<% end %>


  
 


  